@model IEnumerable<OrderPlatform.Models.OrderIndexModel> 

@{
    ViewBag.Title = "Ordini";
}

<h2>Ordini</h2>


<table class="table table-bordered table-responsive table-condensed">
    <thead>
        <tr class="btn-primary">
            <th>@Html.DisplayNameFor(m => m.id)</th>
            <th>date</th>
            <th>username</th>
            <th>state</th>
            <th>edit</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <td>@Html.DisplayForModel(m.id)</td>
                <td>@m.date</td>
                <td>@m.username</td>
                <td>@m.state</td>
                <td><a href="~/Order/Edit/@m.id">Modifica</a>(</td>
            </tr>
        }
    </tbody>
</table>
