@model OrderPlatform.Models.WarehouseProductModel

@using (Html.BeginCollectionItem("warehouseProductList"))
{
    <tr>
        <td>
            @Html.HiddenFor(model => model.id)
            @Html.HiddenFor(model => model.warehouseId)
            @Html.EditorFor(model => model.quantity, new { @class = "control-label col-md-2" })
        </td>
        <td>
            @Html.EditorFor(model => model.productId, new { @class = "control-label col-md-6" })
        </td>
        <td>
            <a href="~/Warehouse/DeleteWarehouseProduct/@Model.id" onclick="return confirm('Are you sure you want to delete this record?')">Delete</a>
        </td>
    </tr>
}
